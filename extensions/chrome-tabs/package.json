    "react": "^18.2.0"
  },
  "dependencies": {
    "@taab-app/api": "^0.0.15",
    "@taab-app/utils": "^0.0.7",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17"
  }
}
